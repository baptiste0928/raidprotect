{
    "upload": {
        "type": "json",
        "features": ["filter_untranslated"],
        "files": {
            "pattern": "raidprotect/locales/*.json",
            "lang": "${autodetectLang}"
        },
        "deprecate": "file"
    },

    "download": {
        "files": "raidprotect/locales/${lang}.json",
        "includeSourceLang": true
    }
  }
